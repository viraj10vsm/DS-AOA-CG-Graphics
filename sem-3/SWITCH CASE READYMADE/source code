#include<stdio.h> 
#include<stdlib.h> 
#include<conio.h> 
struct node 
{ 
};
void create(); 
void Delete(); 
void display(); 
void main() 
{ 
int ch; 
 printf("\n 1. Create List \n 2. Delete List \n 3.Display List \n 4. Exit \n"); 
while(1) 
{ 
printf("\n Enter your choice:"); 
scanf("%d",&ch); 
switch(ch) 
{ 
case 1: create(); 
break; 
case 2: Delete(); 
break; 
case 3: display(); 
break; 
case 4: exit(1); 
default: printf("\n Enter a valid choice!"); 
} 
} 
getch(); 
} 

